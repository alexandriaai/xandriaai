{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "Run Extension (Clean)",
		"type": "extensionHost",
		"request": "launch",
		"preLaunchTask": "npm: compile",
		"args": [
		  "--disable-extensions",
		  "--extensionDevelopmentPath=${workspaceFolder}"
		],
		"outFiles": ["${workspaceFolder}/out/**/*.js"]
	  }
	]
  }
  